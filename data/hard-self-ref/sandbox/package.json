  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch & node --watch dist/agent.js web",
    "start": "node dist/agent.js",
    "cli": "node dist/agent.js cli",
    "web": "node dist/agent.js web",
    "tui": "node dist/agent.js tui",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "db:migrate": "node scripts/migrate.js",
    "clean": "rm -rf dist data/*.json",
    "docker:build": "docker build -t ouroboros:latest .",
    "docker:run": "docker run -d -p 8080:8080 --name ouroboros ouroboros:latest",
    "docker:stop": "docker stop ouroboros && docker rm ouroboros",
    "lint": "echo \"Linting not yet configured\" && exit 0"
  },
  "keywords": [
    "ai",
    "agent",
